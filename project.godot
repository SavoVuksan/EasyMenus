; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

_global_script_classes=[{
"base": "Control",
"class": &"OptionsMenuController",
"language": &"GDScript",
"path": "res://Scripts/options_menu_controller.gd"
}]
_global_script_class_icons={
"OptionsMenuController": ""
}

[application]

config/name="TemplateUI"
run/main_scene="res://Scenes/main_menu.tscn"
config/features=PackedStringArray("4.0")
config/icon="res://icon.svg"

[autoload]

OptionsConstants="*res://Scripts/options_constants.gd"
StartupLoader="*res://Scripts/startup_loader.gd"

[display]

window/size/viewport_width=800
window/size/viewport_height=600
window/size/fullscreen=true

[rendering]

scaling_3d/mode=1
scaling_3d/fsr_sharpness=0.2
