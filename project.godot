; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="TemplateMenus"
config/description="This plugin adds a basic main, options and pause menu to your project."
run/main_scene="res://Templates/MenuTemplates/Scenes/main_menu.tscn"
config/features=PackedStringArray("4.0")
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://Templates/MenuTemplates/default_bus_layout.tres"

[autoload]

OptionsConstants="*res://Templates/MenuTemplates/Scripts/options_constants.gd"
StartupLoader="*res://Templates/MenuTemplates/Scripts/startup_loader.gd"

[display]

window/size/viewport_width=800
window/size/viewport_height=600
window/size/fullscreen=true

[dotnet]

project/assembly_name="TemplateMenus"

[editor_plugins]

enabled=PackedStringArray()

[rendering]

scaling_3d/mode=1
scaling_3d/fsr_sharpness=0.2
